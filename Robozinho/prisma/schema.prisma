// prisma/schema.prisma

datasource db {
  provider = "postgresql"  // Usando PostgreSQL
  url      = env("DATABASE_URL") // Conex√£o no arquivo .env
}

generator client {
  provider = "prisma-client-js"
}

// Model exemplo de tabela User
model Adm {
  id    Int    @id @default(autoincrement())
  name  String
  user String @unique
  password String
  userWtp String
}
